{
	"name" :"open http short link",
	"version": "2",
	"manifest_version": 2,
	"description":"Http短链接反解服务",
	"permissions": ["http://*.crabcloud.com/", "contextMenus"],
	"icons":{
		"16":"icon.jpg",
		"48" : "icon.jpg",
		"128":"icon.jpg"
	},

	"background": {
		"persistent": false,
		"scripts": ["js/contextmenu.js"]
  	},
	"browser_action" : {
		"default_icon" : "icon.jpg",
		"default_title" :"Openlink短链接反解服务",
		"default_popup" : "panel.html"
	},
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
